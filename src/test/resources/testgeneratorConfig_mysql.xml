<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<properties resource="testgeneratorConfig_mysql.properties" />

	<classPathEntry location="${jdbc.path}" />

	<context id="frms-portal" targetRuntime="MyBatis3">
		<plugin type="cn.com.bsfit.frms.portal.base.plugins.mysql.PaginationPlugin" />
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />

		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="${jdbc.driverClassName}"
			connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="cn.com.bsfit.frms.portal.base.pojo"
			targetProject="frms-portal-base/src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="mysql.cn.com.bsfit.frms.portal.base.sqlmap"
			targetProject="frms-portal-base/src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetProject="frms-portal-base" targetPackage="cn.com.bsfit.frms.portal.base.mapper">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		
 		<table schema="frms-portal" tableName="assign_config" domainObjectName="AssignConfig">
